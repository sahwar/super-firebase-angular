{
  "name": "super-firebase-angular",
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:en": "ng serve --configuration=en",
    "start:tr": "ng serve --configuration=tr",
    "build": "ng build",
    "build:i18n": "ng run super-firebase-angular:build:en && ng run super-firebase-angular:build:tr",
    "build:production:i18n": "ng run super-firebase-angular:build:production-en && ng run super-firebase-angular:build:production-tr",
    "server": "ng run super-firebase-angular:server",
    "server:i18n": "ng run super-firebase-angular:server:en && ng run super-firebase-angular:server:tr",
    "server:production:i18n": "ng run super-firebase-angular:server:production-en && ng run super-firebase-angular:server:production-tr",
    "deploy": "firebase deploy --project supermurat-com",
    "test": "ng test",
    "test:cover": "del-cli \"./coverage\" && ng test --watch=false --code-coverage",
    "test:cover:open": "npm run test:cover && opn \"./coverage/index.html\"",
    "lint:all": "npm run lint && npm run lint:functions",
    "lint": "ng lint && npm run stylelint&& npm run lint:html && npm run lint:md",
    "lint:functions": "npm --prefix \"./functions\" run lint",
    "lint:md": "remark .",
    "lint:html": "htmlhint \"src\" --config .htmlhintrc",
    "stylelint": "stylelint \"src/**/*.scss\" --syntax scss",
    "e2e": "ng e2e",
    "e2e:browserstack": "ng e2e --configuration=browserstack",
    "e2e:browserstack:live": "protractor ./e2e/protractor.browserstack.live.conf.js",
    "build:functions": "npm run lint:functions && npm --prefix \"./functions\" run build",
    "serve": "del-cli \"./functions/dist/*\" && npm run build:functions && npm run build && npm run server && npm run copyToPublic && firebase serve --project supermurat-com",
    "serve:i18n": "del-cli \"./functions/dist/*\" && npm run build:functions && npm run build:i18n && npm run server:i18n && npm run copyToPublic && firebase serve --project supermurat-com",
    "serve:production:i18n": "del-cli \"./functions/dist/*\" && npm run build:functions && npm run build:production:i18n && npm run server:production:i18n && npm run copyToPublic && firebase serve --project supermurat-com",
    "serve:only:firebase": "npm run build:functions && firebase serve --project supermurat-com",
    "copyToPublic": "del-cli \"./public/*\" && ncp \"./functions/dist/browser\" \"./public\" && npm run deleteIndexFromPublic",
    "copyCoverageToPublic": "ncp \"./coverage\" \"./public/coverage\"",
    "copyDocumentationToPublic": "ncp \"./documentation\" \"./public/documentation\"",
    "deleteIndexFromPublic": "del-cli \"./public/index.html\" && del-cli \"./public/en/index.html\" && del-cli \"./public/tr/index.html\"",
    "extract:i18n": "ng xi18n --output-path locale && xliffmerge --profile xliffmerge.json",
    "stats": "ng build --stats-json && webpack-bundle-analyzer \"./functions/dist/browser/stats.json\" --host 127.0.0.1 --port 8042",
    "stats:i18n:en": "ng run super-firebase-angular:build:en --stats-json && webpack-bundle-analyzer \"./functions/dist/browser/en/stats.json\" --host 127.0.0.1 --port 8043",
    "stats:i18n:tr": "ng run super-firebase-angular:build:tr --stats-json && webpack-bundle-analyzer \"./functions/dist/browser/tr/stats.json\" --host 127.0.0.1 --port 8044",
    "stats:production:i18n:en": "ng run super-firebase-angular:build:production-en --stats-json && webpack-bundle-analyzer \"./functions/dist/browser/en/stats.json\" --host 127.0.0.1 --port 8045",
    "stats:production:i18n:tr": "ng run super-firebase-angular:build:production-tr --stats-json && webpack-bundle-analyzer \"./functions/dist/browser/tr/stats.json\" --host 127.0.0.1 --port 8046",
    "install:dependencies:global": "npm install -g firebase-tools@6.3.1 @angular/cli@7.3.0 ncp del-cli opn-cli webpack-bundle-analyzer @compodoc/compodoc selenium-webdriver @commitlint/cli",
    "install:dependencies": "npm install && cd functions && npm install",
    "browserslist": "npx browserslist --config=\"./src/browserslist\"",
    "compodoc": "del-cli \"./documentation\" && compodoc -n \"super-firebase-angular documentation\" -p src/tsconfig.app.json --unitTestCoverage ./coverage/coverage-summary.json",
    "compodoc:open": "npm run compodoc && opn \"./documentation/index.html\"",
    "compodoc:cover": "compodoc -p src/tsconfig.app.json --coverageTest 70",
    "commit": "commit-wizard"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.6",
    "@angular/common": "7.2.6",
    "@angular/compiler": "7.2.6",
    "@angular/core": "7.2.6",
    "@angular/fire": "5.1.1",
    "@angular/forms": "7.2.6",
    "@angular/http": "7.2.6",
    "@angular/platform-browser": "7.2.6",
    "@angular/platform-browser-dynamic": "7.2.6",
    "@angular/platform-server": "7.2.6",
    "@angular/pwa": "0.13.3",
    "@angular/router": "7.2.6",
    "@angular/service-worker": "7.2.6",
    "@fortawesome/fontawesome-free": "5.7.2",
    "@ng-bootstrap/ng-bootstrap": "4.0.4",
    "angulartics2": "7.4.1",
    "bootstrap": "4.3.1",
    "classlist.js": "1.1.20150312",
    "core-js": "2.6.5",
    "firebase": "5.8.4",
    "grpc": "1.18.0",
    "protractor": "5.4.2",
    "rxjs": "6.4.0",
    "tslib": "1.9.3",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.3",
    "@angular/cli": "7.3.3",
    "@angular/compiler-cli": "7.2.6",
    "@angular/language-service": "7.2.6",
    "@commitlint/cli": "~7.5.2",
    "@commitlint/config-conventional": "~7.5.0",
    "@types/jasmine": "~3.3.9",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~11.9.5",
    "acorn": "^6.1.0",
    "angular-tslint-rules": "~1.12.1",
    "browserslist": "~4.4.2",
    "browserstack-local": "~1.3.7",
    "codacy-coverage": "~3.4.0",
    "codelyzer": "~4.5.0",
    "coveralls": "~3.0.3",
    "husky": "~1.3.1",
    "htmlhint": "~0.11.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jquery": "~3.3.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.0",
    "karma-mocha-reporter": "~2.2.5",
    "karma-scss-preprocessor": "4.0.0",
    "ngx-i18nsupport": "~0.17.1",
    "node-sass": "~4.11.0",
    "popper.js": "~1.14.7",
    "remark-cli": "~6.0.1",
    "remark-lint": "~6.0.4",
    "remark-preset-lint-markdown-style-guide": "~2.1.2",
    "remark-preset-lint-recommended": "^3.0.2",
    "stylelint": "~9.10.1",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.5.4",
    "terser": "3.16.1",
    "ts-node": "~8.0.2",
    "tslint": "5.13.0",
    "typescript": "3.2.4",
    "webpack": "~4.29.5",
    "webpack-bundle-analyzer": "~3.0.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "echo 'Pre-commit: running lint...' && npm run lint && npm run lint:functions"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
